{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/Accordion/FAQAccordionPage/FAQAccordionPage.jsx", "app/redux/features/auth-slice.ts", "components/Header/page.jsx", "components/HeaderProfile/HeaderProfile.jsx", "app/story/page.jsx", "app/redux/features/trac-get.js"],
  "exclude": ["node_modules"]
}
